{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FeatureManagement": {
    "ModuleA": {
      "EnabledFor": [
        {
          "Name": "AppVersion",
          "Parameters": {
            "Versions": {
              "Stable": "Enabled",
              "Beta": "Enabled"
            }
          }
        }
      ]
    },
    "ModuleB": {
      "EnabledFor": [
        {
          "Name": "AppVersion",
          "Parameters": {
            "Versions": {
              "Stable": "OptIn",
              "Beta": "Enabled"
            }
          }
        }
      ]
    },
    "ModuleC": {
      "EnabledFor": [
        {
          "Name": "AppVersion",
          "Parameters": {
            "Versions": {
              "Stable": "Disabled",
              "Beta": "OptIn"
            }
          }
        }
      ]
    },
    "ModuleD": {
      "EnabledFor": [
        {
          "Name": "UserPercentage",
          "Parameters": {
            "Percentage": 50
          }
        }
      ]
    }
  }
}
