{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FeatureManagement": {
    "ModuleA": {
      "EnabledFor": [
        {
          "Name": "AppVersion",
          "Parameters": {
            "Versions": ["Stable"]
          }
        }
      ]
    },
    "ModuleB": {
      "EnabledFor": [
        {
          "Name": "OptIn"
        }
      ]
    }
  }
}
